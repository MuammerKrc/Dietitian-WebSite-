                                                        <div class="card-title Bilgile text-center  p-2"
                                                    style="position: relative;">
                                                            <h4 class="text-warning">Diyet</h4>
                                                            <hr style="color: red;">
                                                            <div class="bg-danger text-white">Tanımlanmamış</div>
                                                        </div>
                                                    }
                                                    @if (item.Pilates != null)
                                                    {
                                                        <div class="card-title Bilgile text-center  p-2"
                                                    style="position: relative;">
                                                            <a href="~/admin/CustomerPilates/?customerId=@item.Id">
                                                                <h4 class="text-warning ">Pilates</h4>
                                                                <hr style="color: red;">
                                                                <div
                                                            class=" @(item.RemaningPilatesPackages<1?"bg-warning":"bg-light")">
                                                                    @item.RemaningPilatesPackages</div>
                                                            </a>
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="card-title Bilgile text-center  p-2"
                                                    style="position: relative;">
                                                            <h4 class="text-warning">Pilates</h4>
                                                            <hr style="color: red;">
                                                            <div class="bg-danger  text-white">Tanımlanmamış</div>
                                                        </div>
                                                    }
                                                </div>
                                                <div class="card-footer">
                                                    <h5 class="text-muted" style="font-size:12px;"><i
                                                        class="fas fa-phone-square-alt mx-2"></i>Telefon</h5>
                                                    <small class="telSearch text-muted" style="font-size:10px;">@(item.PhoneNumber==null?"Telefon Girilmemiştir":item.PhoneNumber)</small>
                                                    <hr style="width: 50%; color: red;">
                                                    <h5 class=" text-muted" style="font-size:12px;"><i
