                                        </div>
                                    </div>
                                    <hr>
                                    <div class="row make-flex-row space-around">
                                         @foreach (var item in Model)
                                        {
                                             <div id="AllClassSearch" class="card  g-0 shadow  bg-body rounded mt-2 mx-1" style="width:200px;">
                                                <div class="card-header"
                                                style="position: relative;  background-color:rgba(32, 39, 49,0.5)">
                                                    <a href="~/admin/CustomerHome/@item.Id">
                                                        <p  class="NameSearch h4 text-white text-center">@item.FirstName <br> @item.LastName </p>
