@model List<Customer>

@section css{
<link rel="stylesheet" href="~/css/online.css">
    
}


<div class="content-wrapper" style="margin-top: 50px;">

    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Admin AnaSayfa</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item">Admin AnaSayfa</li>
                    </ol>
                </div>
            </div>
        </div>
    </section>

    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-3">
                    <div class="card card-primary card-outline">
                        <div class="card-body box-profile">
                            <div class="text-center">
                                <img class="profile-user-img img-fluid " src="~/avatar/avatar2.png"
                                    alt="User profile picture">
                            </div>
                            <h3 class="profile-username text-center makeUpperCase">Admin</h3>
                            <div class="row make-flex-row space-around my-3">

                                <a href="~/admin/DietMenüList"
                                    class="btn btn-secondary btn-block col-5 col-sm-3 my-2 col-md-5"><b>Menüler</b></a>

                                <a href="~/RoleAndUser/UserList"
                                    class="btn btn-secondary btn-block col-5 col-sm-3 my-2 col-md-5"><b>Kullanıcılar</b></a>

                                <a href="~/RoleAndUser/RoleList"
                                    class="btn btn-secondary btn-block col-5 col-sm-3 my-2 col-md-5"><b>Rolleme</b></a>
                            </div>
                            @if (ViewBag.AllDateRequest != null)
                            {
                                <div class="card g-0" style="width:100%;">
                                    <h5 class="card-header text-center ">Randevu Talebi</h5>
                                    <div class="card-body  make-flex-row space-beetwen">
                                        <table class="table table-hover table-light" style="font-size: 10px;">
                                            <thead>
                                                <tr>
                                                    <th>İstek</th>
                                                    <th>Adı</th>
                                                    <th>Tarih</th>
                                                    <th></th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var item in (List<DateRequest>)ViewBag.AllDateRequest)
                                                {
                                                    <tr>
                                                        <td>@item.typeOfRequest.ToString()</td>
                                                        <td>@item.FullName</td>
                                                        <td>@item.RequestTİme</td>
                                                        <td>
                                                            @if (item.typeOfRequest == DateType.Diet)
                                                            {
                                                                <a href="~/admin/CustomerWeek/@item.WeekId"
                                                        class="btn btn-success btn-sm">Git</a>
                                                                <a href="~/admin/RemoveDateRequest/@item.Id"
                                                        class="btn btn-danger btn-sm">Sil</a>
                                                            }
                                                            else if (item.typeOfRequest == DateType.Pilates)
                                                            {
                                                                <a href="~/admin/CustomerPilatesWeek/@item.WeekId"
                                                        class="btn btn-success btn-sm">Git</a>
                                                                <a href="~/admin/RemoveDateRequest/@item.Id"
                                                        class="btn btn-danger btn-sm">Sil</a>
                                                            }
                                                            else
                                                            {
                                                                <a href="#"
                                                        class="btn btn-success btn-sm">Bilinmiyor</a>
                                                                <a href="~/admin/RemoveDateRequest/@item.Id"
                                                        class="btn btn-danger btn-sm">Sil</a>
                                                            }

                                                        </td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            }
                            else
                            {
                                <div class="alert alert-success">
                                    Randevu İsteği Bulunmamaktadır
                                </div>
                            }
                            @if (ViewBag.AllPackageRequest != null)
                            {
                                <div class="card g-0" style="width:100%;">
                                    <h5 class="card-header text-center ">Paket İstekleri</h5>
                                    <div class="card-body   make-flex-row space-beetwen">
                                        <table class="table table-hover table-dark" style="font-size: 10px;">
                                            <thead>
                                                <tr>
                                                    <th>İstek</th>
                                                    <th>Adı</th>
                                                    <th>Phone</th>
                                                    <th>Tarih</th>
                                                    <th></th>
                                                </tr>
                                            </thead>
                                            <tbody>

                                                @foreach (var item in (List<PackageRequest>)ViewBag.AllPackageRequest)
                                                {
                                                    <tr>
                                                        <td>@item.typeOfRequest.ToString()</td>
                                                        <td>@item.FullName</td>
                                                        <td>@item.PhoneNumber</td>
                                                        <td>@item.RequestTime</td>
                                                        <td>
                                                            <a href="~/admin/CustomerHome/@item.CustomerId"
                                                        class="btn btn-success btn-sm">Git</a>
                                                            <a href="~/admin/DeletePackageRequest/@item.Id"
                                                        class="btn btn-danger btn-sm">Sil</a>
                                                        </td>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            }
                            else
                            {
                                <div class="alert alert-success">
                                    Paket İsteği Bulunmamaktadır
                                </div>
                            }
                        </div>
                    </div>

                </div>
                <div class="col-md-9">
