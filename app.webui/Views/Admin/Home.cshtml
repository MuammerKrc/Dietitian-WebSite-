                                            </div>
                                        }
                                    </div>
                                </div>

                                <div class="tab-pane" id="info">
                                    <div class="col-md-8">
                                        <form class="form-horizontal" method="POST" asp-controller="admin"
                                            asp-action="GeneralMesaj">
                                            <div class="form-group row mt-2">
                                                <label for="Title" class="col-sm-2 col-form-label">Başlık</label>
                                                <div class="col-sm-10">
                                                    <input type="text" class="form-control" id="Title" name="Title"
                                                        placeholder="Başlık" required minlength="5">
                                                </div>
                                            </div>
                                            <div class="form-group row mt-2">
                                                <label for="Content" class="col-sm-2 col-form-label">İçerik</label>
                                                <div class="col-sm-10">
                                                    <textarea class="form-control" id="Content" name="Content"
                                                        placeholder="İçerik" required minlength="10"></textarea>
                                                </div>
                                            </div>
                                            <div class="form-group row mt-2">
                                                <label for="Url" class="col-sm-2 col-form-label">Url</label>
                                                <div class="col-sm-10">
                                                    <input type="text" class="form-control" id="Url" name="Url"
                                                        placeholder="Url" minlength="15">
                                                </div>
                                            </div>
                                            <div class="offset-sm-2 input-group mb-3">
                                                <div class="input-group-prepend">
                                                    <label class="input-group-text" for="inputGroupSelect01">Renk
                                                        Secimi</label>
                                                </div>
                                                <select class="custom-select" id="inputGroupSelect01"
                                                    name="TypeOfAlert">
                                                    <option value="0">Primary</option>
                                                    <option value="1">Secondary</option>
                                                    <option value="2">Success</option>
                                                    <option value="3">Danger</option>
                                                    <option value="4">Warning</option>
                                                    <option value="5">İnfo</option>
                                                    <option value="6">Light</option>
                                                    <option value="7">Dark</option>
                                                </select>
                                            </div>
                                            <div class="form-group row mt-2">
                                                <div class="offset-sm-2 col-sm-10">
                                                    <div class="checkbox">
                                                        <label>
                                                            <input type="checkbox" value="1" name="deneme">Url var mı ?
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group row mt-2">
                                                <div class="offset-sm-2 col-sm-10">
                                                    <button type="submit" class="btn btn-danger">Submit</button>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                    <div class="col-12">
                                        <table class="table table-striped table-hover">
                                            <thead>
                                                <tr>
                                                    <th>Tarih</th>
                                                    <th>Title</th>
                                                    <th>Content</th>
                                                    <th>Sil</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @if (ViewBag.General != null)
                                                {
                                                    @foreach (var item in (List<GeneralMesaj>)ViewBag.General)
                                                    {
                                                        <tr>
                                                            <th>@item.Time</th>
                                                            <th>@item.Title </th>
                                                            <th>@item.Content</th>
                                                            <th><a href="~/admin/RemoveGeneralMesaj/@item.Id"
                                                            class="btn btn-dark btn-sm">Sil</a></th>
                                                        </tr>
                                                    }
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <div class="tab-pane" id="Recipe">
                                    <div class="col-md-8">
                                        <form class="form-horizontal" method="POST" asp-controller="admin"
                                            asp-action="CreateRecipe">
                                            <div class="form-group row mt-2">
                                                <label for="Name" class="col-sm-2 col-form-label">Tarif Adı</label>
                                                <div class="col-sm-10">
                                                    <input type="text" class="form-control" id="Name" name="Name"
                                                        placeholder="Tarif Adı" required minlength="3">
                                                </div>
                                            </div>
                                            <div class="form-group row mt-2">
                                                <label for="Content" class="col-sm-2 col-form-label">İçerik</label>
                                                <div class="col-sm-10">
                                                    <textarea class="form-control" id="Content" name="Content"
                                                        placeholder="İçerik" required minlength="10"></textarea>
                                                </div>
                                            </div>

                                            <div class="form-group row mt-2">
                                                <div class="offset-sm-2 col-sm-10">
                                                    <button type="submit" class="btn btn-danger">Kaydet</button>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                    <div class="col-6">
                                        <table class="table table-striped table-hover">
                                            <thead>
                                                <tr>
                                                    <th>Tarif Adı</th>
                                                    <th>Tarif İçerik</th>
                                                </tr>
                                            </thead>
                                            <tbody>
