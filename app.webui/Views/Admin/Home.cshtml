                                                    <small class="mailSearch text-muted"
                                                    style="font-size:10px;">@(item.Mail==null?"Mail Gİrilmemiştir":item.Mail)</small>
                                                </div>
                                            </div>
                                            }
                                        }
                                    </div>
                                </div>

                                <div class="tab-pane" id="tüm">
                                     <div class="form-group row my-2">
                                        <label for="inputName" class="col-sm-2 col-form-label">Adı</label>
                                        <div class="col-sm-10">
                                            <input  type="text" onkeyup="ForName(this)" class="form-control"  placeholder="Name">
                                        </div>
                                    </div>
                                    <div class="form-group row my-2">
